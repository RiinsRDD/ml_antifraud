{
  "output": {
    "filePath": "repomix-output.md",
    "style": "markdown",
    "removeComments": false,
    "showLineNumbers": true
  },
  "include": [
    "**/*"
  ],
  "ignore": {
    "customPatterns": [
      "**/venv/**",
      "**/.venv/**",
      "**/__pycache__/**",
      "**/.pytest_cache/**",
      "**/.mypy_cache/**",
      "**/.git/**",
      "**/migrations/**",
      "*.log",
      "*.pyc",
      "README.md",
      "*repomix.config.json",
      ".gitignore",
      "uv.lock",
      "*.egg-info",
      "*.html"
    ]
  },
  "security": {
    "enableCheck": true
  }
}
